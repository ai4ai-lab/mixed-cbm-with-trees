{
    "name": "ind_cbm_mimic_binary",
    "n_gpu": 1,

    "arch": {
        "type": "MIMICCBMArchitecture",
        "args": {}
    },
    "model": {
        "xc_lr": 0.001,
        "xc_weight_decay": 0,
        "cy_lr": 0.01,
        "cy_weight_decay": 0,
        "pretrained_concept_predictor": "./trained_models/pretrained_xtoc_encoders/mimic_binary/0928_230221/model_best.pth"
    },
    "dataset": {
        "num_classes": 2,
        "num_concepts": 18,
        "concept_names": {
            "respiration_normal": ["False", "True"],
            "respiration_moderate": ["False", "True"],
            "respiration_severe": ["False", "True"],
            "renal_normal": ["False", "True"],
            "renal_moderate": ["False", "True"],
            "renal_severe": ["False", "True"],
            "coagulation_normal": ["False", "True"],
            "coagulation_moderate": ["False", "True"],
            "coagulation_severe": ["False", "True"],
            "liver_normal": ["False", "True"],
            "liver_moderate": ["False", "True"],
            "liver_severe": ["False", "True"],
            "cardiovascular_normal": ["False", "True"],
            "cardiovascular_moderate": ["False", "True"],
            "cardiovascular_severe": ["False", "True"],
            "cns_normal": ["False", "True"],
            "cns_moderate": ["False", "True"],
            "cns_severe": ["False", "True"]
        },
        "class_names": ["recovered", "passed away"],
        "use_attribute_imbalance": false
    },
    "data_loader": {
        "type": "get_mimic_dataLoader",
        "args":{
            "data_dir": "",
            "type": "SGD",
            "batch_size": 206
        }
    },
    "trainer": {
        "type": "IndependentCBMTrainer",
        "hard_cbm": true,
        "args": {},
        "xc_epochs": 100,
        "cy_epochs": 200,
        "save_dir": "saved/",
        "save_period": 1000,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 0,
        "tensorboard": true,
        "save_test_tensors": false
    },
    "regularisation": {
        "type": "None",
        "min_samples_leaf": 30,
        "snapshot_epochs": 50
    }
}
